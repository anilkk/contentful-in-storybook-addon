{
  "name": "Contentful in storybook",
  "version": "1.0.0",
  "description": "A Storybook addon to design with Contentful data",
  "main": "dist/preset.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook"
  },
  "files": ["dist/**/*", "README.md", "*.js"],
  "keywords": ["storybook", "addons", "contentful"],
  "storybook": {
    "displayName": "Contentful in Storybook",
    "unsupportedFrameworks": ["Vue",  "Angular"],
    "supportedFrameworks": ["React"],
    "icon": "https://yoursite.com/outline-icon.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/someone/my-addon"
  },
  "author": "Anil Kumar Krishnashetty",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "babel-loader": "^8.2.2"
  }
}
